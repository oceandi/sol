RewriteEngine On

# WWW'den WWW olmayan versiyona y√∂nlendirme
RewriteCond %{HTTP_HOST} ^www\.superonline-deha\.net$ [NC]
RewriteRule (.*) https://superonline-deha.net/$1 [R=301,L]

# index.html'i gizleme
